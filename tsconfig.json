{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "isolatedModules": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "importsNotUsedAsValues": "remove",
    "paths": {
      "@/*": ["./src/*"]
    },
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
